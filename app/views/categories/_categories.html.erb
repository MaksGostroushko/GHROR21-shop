<section class="category-wrap">
    <div class="row no-gutters">
      <% Category.all.each do |category| %>
        <div class="col-sm-12 col-lg-4">
          <div class="categories-list">
            <p>all for:</p>
          <%= link_to category.name, category_path(category) %>
          </div>
        </div>
      <% end %>
    </div>
</section>
